@section MetaTags {
    <meta name="keywords" content="Yamaha MT-09, Yamaha MT-09 price, MT-09 launch date, Yamaha big bikes, inline-three motorcycle, MT-09 specs, Yamaha MT-09 features, BikeCarHub, Kawasaki Z900, Triumph Street Triple RS, Ducati Monster." />
    <meta name="description" content="The Yamaha MT-09 is back! Explore its expected launch date, price, and detailed specifications. A true Master of Torque is set to return." />
}
<!--For truncate-->
@functions {
    public string Truncate(string input, int length)
    {
        if (string.IsNullOrEmpty(input) || input.Length <= length)
            return input;
        return input.Substring(0, length) + "...";
    }
}
