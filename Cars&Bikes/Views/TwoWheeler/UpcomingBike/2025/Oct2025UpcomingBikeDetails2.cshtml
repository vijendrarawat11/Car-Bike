@section MetaTags {
    <meta name="keywords" content="Yezdi Streetfighter, Yezdi Streetfighter price, Yezdi bike, upcoming Yezdi bikes, Yezdi Streetfighter launch, Yezdi 334, Streetfighter motorcycle, BikeCarHub, Yezdi Streetfighter specs, Honda CB350RS, Royal Enfield Scram 411." />
        <meta name="description" content="Discover the new Yezdi Streetfighter's expected price, launch date, and detailed specifications. A perfect blend of classic styling and modern performance." />
}
<!--For truncate-->
@functions {
    public string Truncate(string input, int length)
    {
        if (string.IsNullOrEmpty(input) || input.Length <= length)
            return input;
        return input.Substring(0, length) + "...";
    }
}
