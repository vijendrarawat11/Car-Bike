@section MetaTags {
    <meta name="keywords" content="Triumph Speed 400 Tracker, Triumph Speed 400 Tracker price, Triumph 400cc, Triumph bikes India, upcoming Triumph bikes, flat tracker motorcycle, Triumph launch, BikeCarHub, Triumph Speed 400 Tracker specs, Royal Enfield Scram 440,bikecarhub" />
    <meta name="description" content="Explore the new Triumph Speed 400 Tracker at bikecarhub. Find out its launch date, expected price, and detailed specifications. A perfect blend of style and performance." />
}
<!--For truncate-->
@functions {
    public string Truncate(string input, int length)
    {
        if (string.IsNullOrEmpty(input) || input.Length <= length)
            return input;
        return input.Substring(0, length) + "...";
    }
}
