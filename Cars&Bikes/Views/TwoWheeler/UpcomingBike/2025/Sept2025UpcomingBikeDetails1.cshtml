@{
    ViewData["Title"] = "Benelli TNT600i: The Inline-4 Roar Returns";
}
@section MetaTags {
    <meta name="keywords" content="Benelli TNT600i, Benelli TNT 600i price, TNT600i launch date, Benelli 600cc bike, Benelli naked motorcycle, inline-4 motorcycle, Benelli TNT600i specs, Benelli TNT600i competitors, BikeCarHub, upcoming bikes India, middleweight naked bikes,bikecarhub" />
    <meta name="description" content="Get ready for the new Benelli TNT600i. Discover its expected price, launch date, specifications, and a detailed comparison with its rivals at bikecarhub." />
}
<!--For truncate-->
@functions {
    public string Truncate(string input, int length)
    {
        if (string.IsNullOrEmpty(input) || input.Length <= length)
            return input;
        return input.Substring(0, length) + "...";
    }
}
