@model string

@if (Model == "TwoWheeler")
{
    <a class="btn btn-primary mt-3" href="/TW/Create">Add Another Two Wheeler</a>
}
else if (Model == "TWVarient")
{
    <a class="btn btn-primary mt-3" href="/TW/CreateTWVarient">Add Another Variant</a>
}
else
{
    <a class="btn btn-secondary mt-3" href="/AdminLogin/Login">Go Home</a>
}
