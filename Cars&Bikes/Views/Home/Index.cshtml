<!--Upcoming Bike-->
<script>
    document.addEventListener("DOMContentLoaded", function () {
        const container = document.getElementById('upcomingBikesContainer');
        const itemWidth = container.querySelector('.upcoming-bike-item').offsetWidth;

        function scrollLeft() {
            container.scrollBy({
                left: -itemWidth,
                behavior: 'smooth'
            });
        }

        function scrollRight() {
            container.scrollBy({
                left: itemWidth,
                behavior: 'smooth'
            });
        }

        document.querySelector('.scroll-button.left').addEventListener('click', scrollLeft);
        document.querySelector('.scroll-button.right').addEventListener('click', scrollRight);
    });
</script>
<!--Latest New-->
<script>
    document.addEventListener("DOMContentLoaded", function () {
        function scrollLeft(containerId) {
            const container = document.querySelector(`#${containerId} .latest-news-container`);
            const itemWidth = container.querySelector('.latest-news-item').offsetWidth;
            container.scrollBy({
                left: -itemWidth,
                behavior: 'smooth'
            });
        }

        function scrollRight(containerId) {
            const container = document.querySelector(`#${containerId} .latest-news-container`);
            const itemWidth = container.querySelector('.latest-news-item').offsetWidth;
            container.scrollBy({
                left: itemWidth,
                behavior: 'smooth'
            });
        }

        document.querySelectorAll('.navlatestnews a').forEach(tab => {
            tab.addEventListener('click', function (e) {
                e.preventDefault();

                document.querySelectorAll('.navlatestnews a').forEach(link => {
                    link.classList.remove('active');
                });

                this.classList.add('active');

                document.querySelectorAll('.tab-lscontent').forEach(pane => {
                    pane.classList.remove('active');
                });

                const target = this.getAttribute('href');
                document.querySelector(target).classList.add('active');
            });
        });

        document.querySelectorAll('.scroll-btn.left').forEach(button => {
            button.addEventListener('click', function () {
                const containerId = this.closest('.tab-lscontent').id;
                scrollLeft(containerId);
            });
        });

        document.querySelectorAll('.scroll-btn.right').forEach(button => {
            button.addEventListener('click', function () {
                const containerId = this.closest('.tab-lscontent').id;
                scrollRight(containerId);
            });
        });
    });
</script>
@*--------Slide-------- *@
<script>
    let currentIndex = 0;

    function showSlide(index) {
        const slides = document.querySelector('.slides');
        const totalSlides = document.querySelectorAll('.slide').length;

        if (index >= totalSlides) {
            currentIndex = 0;
        } else if (index < 0) {
            currentIndex = totalSlides - 1;
        } else {
            currentIndex = index;
        }

        slides.style.transform = `translateX(-${currentIndex * 100}%)`;
    }

    function nextSlide() {
        showSlide(currentIndex + 1);
    }

    setInterval(nextSlide, 5000); // Change image every 5 seconds

    // Initial call to show the first slide
    showSlide(currentIndex);
</script>