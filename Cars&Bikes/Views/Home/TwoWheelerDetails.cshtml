                    } else {
                        // Handle case where no data is returned
                        $('#IdTWimage').attr('src', ''); // Set to a default or empty image
                        $('#IdTWcolor').text('N/A');
                        $('#IdTWLaunchdate').html('N/A');
                    }
                }).fail(function (xhr, status, error) {
                    console.error(xhr.responseText);
                });
            }

            // Trigger specs fetch on page load
            var initialVarientId = $('#varientId').val();
            if (initialVarientId) {
                fetchSpecs(initialVarientId);
            }
        });
    </script>
</body>