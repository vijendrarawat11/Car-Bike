@{
    ViewData["Title"] = "KTM Duke 200 vs Bajaj Pulsar NS200 – Full Comparison";
}
@section MetaTags {
    <meta name="keywords" content="KTM Duke 200 vs Bajaj Pulsar NS200, Duke 200 mileage, Pulsar NS200 features, KTM Duke 200 price, Pulsar NS200 performance, best 200cc bikes India 2025, KTM Duke 200 specs, Pulsar NS200 comparison" />
    <meta name="description" content="Compare KTM Duke 200 and Bajaj Pulsar NS200 for mileage, features, price, and daily comfort. Find the perfect 200cc street bike for you at BikeCarHub." />
}

@functions {
    public string Truncate(string input, int length)
    {
        if (string.IsNullOrEmpty(input) || input.Length <= length)
            return input;
        return input.Substring(0, length) + "...";
    }
}
