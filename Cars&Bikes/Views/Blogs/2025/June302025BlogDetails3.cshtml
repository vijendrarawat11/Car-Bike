@{
    ViewData["Title"] = "TVS Jupiter vs Honda Activa: 2025 Scooter Comparison";
}
@section MetaTags {
    <meta name="keywords" content="TVS Jupiter vs Honda Activa comparison 2025, Honda Activa mileage 2025, TVS Jupiter features 2025, Best scooters in India 2025, Activa vs Jupiter which is better, TVS Jupiter comfort review 2025, Honda Activa resale value 2025, Top commuter scooters 2025 India, TVS Jupiter mileage comparison, Honda Activa vs Jupiter specifications, Scooters with external fuel fill India, TVS Jupiter vs Activa maintenance cost, Best scooter for daily use 2025, Honda Activa 2025 features and price, TVS Jupiter service and performance review" />
    <meta name="description" content="Compare TVS Jupiter vs Honda Activa in this 2025 guide. Get detailed specs, features, mileage, and expert recommendations to choose your ideal scooter." />
}

@functions {
    public string Truncate(string input, int length)
    {
        if (string.IsNullOrEmpty(input) || input.Length <= length)
            return input;
        return input.Substring(0, length) + "...";
    }
}
<body id="bg">
    @* <div class="header-img"><img src="~/images/main-slider/header.png" alt="bikecarhubheaderimage" title="bikecarhubheaderimage" /></div> *@
