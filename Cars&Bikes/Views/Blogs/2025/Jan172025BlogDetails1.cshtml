@{
    ViewData["Title"] = "Toyota Taisor vs Maruti Fronx - Design, Features, Performance, and Price Comparison";
}
@section MetaTags {
    <meta name="keywords" content="Toyota Taisor, Maruti Fronx, subcompact SUVs, SUV comparison, Toyota vs Maruti, Taisor design, Fronx features, Taisor pricing, Fronx performance, affordable SUVs" />
    <meta name="description" content=": Explore the comprehensive comparison between Toyota Taisor and Maruti Fronx. Compare design, features, engine performance, safety, and pricing to choose your ideal subcompact SUV." />
}
<!--For truncate-->
@functions {
    public string Truncate(string input, int length)
    {
        if (string.IsNullOrEmpty(input) || input.Length <= length)
            return input;
        return input.Substring(0, length) + "...";
    }
}
<body id="bg">
