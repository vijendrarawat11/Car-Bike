<head>
    <meta charset="UTF-8">
    <title>Bootstrap Horizontal Scroll Cards</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
    <style>

        .compare-scroll-container {
            position: relative;
            width: 100%;
        }

        .compare-scroll-btn {
            position: absolute;
            top: 50%;
            transform: translateY(-50%);
            background-color: rgba(255, 255, 255, 0.8);
            border: none;
            padding: 10px;
            cursor: pointer;
            pointer-events: all;
            z-index: 1000;
            transition: background 0.3s ease;
        }


        .compare-scroll-left {
            left: 10px;
        }

        .compare-scroll-right {
            right: 10px;
        }

        .card-wrapper {
            display: flex;
            overflow-x: auto;
            scroll-behavior: smooth;
            gap: 1rem;
            /* padding: 1rem 0;  */
            margin: 0;
        }

            .card-wrapper::-webkit-scrollbar {
                display: none;
            }

        .card {
            flex: 0 0 auto;
            width: 33.33%;
            min-width: 300px;
            height: 250px;
            box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);
            border-radius: 8px;
            overflow: hidden;
            margin-bottom: 20px;
        }

        .compare-card {
            height: 150px;
            cursor: pointer;
        }

        .compare-scroll-btn,
        .compare-scroll-btn:focus,
        .compare-scroll-btn:active,
        .compare-scroll-btn:hover {
            background-color: rgba(255, 255, 255, 0.8);
            outline: none;
            box-shadow: none;
        }

            .compare-scroll-btn:focus {
                outline: 2px solid #555;
            }

        .card-img-top {
            width: 100%;
            height: 200px;
            object-fit: cover;
        }

        .card-body {
            padding: 0.5rem;
            text-align: center;
        }

        .card-wrapper .card:first-child {
            margin-left: 0.5rem;
        }

        .card-wrapper .card:last-child {
            margin-right: 1rem;
        }

        .card-body {
            text-align: center;
            padding: 1rem;
        }

        .compare-section {
            padding: 40px 20px; 
        }


